{
    "expo": {
        "name": "kanok_appV2",
        "slug": "kanok_appV2",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/logo-black.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/loading.png",
            "resizeMode": "cover",
            "backgroundColor": "#FF7FA8"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.test.test"
        },
        "android": {
            "permissions": ["INTERNET"],
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/logo-black.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.homiez09.kanok_appV2"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/logo-black.png"
        },
        "plugins": [
            "expo-router",
            "expo-image-picker"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "eas": {
                "projectId": "47d5a095-cf04-4d51-b8dd-60b046171695"
            }
        },
        "owner": "homiez09"
    }
}